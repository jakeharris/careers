---
title: Student Workers | Career Center
name: student-workers
type: staff
layout: no-social.hbs
---

<div class="content content--no-padding"><a href="aboutus/staff.html" class="flat-btn flat-btn--white">Back to Full-time Staff</a></div>

{{#each student-workers.category}}
  <div class="content extra-content">
    <h2>{{this}}</h2>
  </div>
  <div class="staff-container">
    {{#each ../student-workers.member}}
      {{#is category ../this}}
        <section class="contact-card">
          <div class="img-liaison-container">
            {{#if img}}
            <img class="{{img}}" src="assets/images/staff/{{img}}.jpg">
            {{/if}}
          </div>
          <div class="contact-info">
            <h2>{{firstName}}</br>{{lastName}}</h2>
            {{#if position}}<h3>{{position}}</h3>{{/if}}
            {{#if major}}<h3>{{major}}</h3>{{/if}}
            {{#if year}}<h3>{{year}}</h3>{{/if}}
          </div>
        </section>
      {{/is}}
    {{/each}}
  </div>
{{/each}}