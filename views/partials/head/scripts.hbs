<!-- Angular & Core JS -->
{{#if component }}
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js" defer></script>
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-route.min.js" defer></script>
{{/if}}
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js" defer></script> 
<script src="http://netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js" defer></script> 
<script src="//cdn.auburn.edu/assets/js/default.min.js" defer></script>

<!-- Application-specific CSS/JS -->
<!-- build:js assets/scripts/{{ name }}.min.js -->
  <script src="assets/scripts/components/nav.{{#if production}}min.{{/if}}js" defer></script>
  <script src="assets/scripts/components/touch-start.{{#if production}}min.{{/if}}js" defer></script>
  {{#each component}}
    <script src="assets/scripts/components/{{ this }}.{{#if production}}min.{{/if}}js" defer></script>
    {{#is this 'sponsors'}}
      <script src="bower_components/angular-lazy-img/release/angular-lazy-img.min.js" defer></script>
    {{/is}}
  {{/each}}
  <script src="assets/scripts/{{ type }}.{{#if production}}min.{{/if}}js" defer></script>
<!-- endbuild -->